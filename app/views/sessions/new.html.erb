<h1> Log In </h1>

<!-- <%= form_for(@user) do |f| %> -->
	<%= form_tag("/sessions", method: "post") do %>
	  <div class='field'>
	    	<%= f.label :username %><br>
	      <%= f.text_field(:username, placeholder: '...username') %>
	  </div>
	  <div class='field'>
	    <%= f.label :password %><br>
	    <%= f.password_field(:password, placeholder: '...password') %>
	    <%= f.submit("Login") %>
	    </div>
	<% end %>
<!-- <% end %> -->