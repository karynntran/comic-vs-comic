<div class="six columns" id="navigation">		
	<div class="five columns" id="nav-user">
		<% if current_user %>
			You are logged in as <%=current_user.username%>
	</div>
			<div class="one column" id="logout">
				<%= form_tag sessions_path, method: 'delete' do %>
				  <%= submit_tag 'Logout', id: 'nav-buttons' %>
				<% end %>
			</div>
	<div class="one-half">
		<% else %>
			<%= link_to 'Log In', login_path, id: 'nav-buttons' %>
	</div>
	<div class="one-half">
		<%= link_to 'Sign Up', new_user_path, id: 'nav-buttons' %>
		<% end %>
	</div>
</div>


